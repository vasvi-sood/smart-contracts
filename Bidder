pragma solidity >=0.7.0 <0.8.0;
contract Bidder
{
    string public name;
    uint public bidamount;
    uint public minbid=10000;
    bool public eligible;
    constructor(string memory n)
    {
       name=n; 
    }
    function setBid(uint bid) public {
        bidamount=bid;
    }
    function get() public view returns(uint this.bidamount)
    {
        
    }
    function isEligible() public returns(string memory,bool)
    {
    if(bidamount>minbid)
    eligible=true;
    else
    eligible=false;
    string memory hello;
    return (hello,eligible);
   
    }
}
